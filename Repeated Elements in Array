#include <stdio.h>
int main()
{
    int n,i,j;
    printf("Enter n:");
    scanf("%d",&n);
    int arr[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        printf(" \nArray elements is: %d",arr[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++){
        if(arr[i]==arr[j])
        {
            printf("\nRepeated element is:%d",arr[i]);
        }
        }
    }
    return 0;
}
